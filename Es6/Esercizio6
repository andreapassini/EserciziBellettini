git init

printf "uno\n" > U
printf "due\n" > Z

git add U Z

git commit -m primo

mkdir A

printf "tre\n" > B
git mv U A

git add B A (forse basta solo add B)

git commit -m bello

git branch ok
git switch ok

printf "aggiunta\n" >> Z (append)

git add Z (forse non serve)

git commit -m ultimo

git switch master
